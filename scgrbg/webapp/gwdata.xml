<公文封装包>
	<!-- 可以有多个封装实体 -->
	<封装实体>
		<发文机关>
			<身份标识>22</身份标识>
			<身份名称>信息办</身份名称>
			<!-- 该节点可选 -->
			<身份描述></身份描述>
		</发文机关>
		<收文机关>
			<身份标识>101-3,102-3,15</身份标识>
			<身份名称>黄山市直单位,安庆市市委办,文书处</身份名称>
			<!-- 该节点可选 -->
			<身份描述></身份描述>
		</收文机关>
		<公文信息>
			<密级>非密mj</密级>
			<紧急程度>一般jjcd</紧急程度>
			<发文机关>拟稿单位ngdw</发文机关>
			<收文机关>文件主送单位zs</收文机关>
			<公文标题>公文处理系统转交换系统文件bt</公文标题>
			<公文内容>正式件X.ofd文件的二进制流（暂取编号最大的）select * from gw_dzwj where con_table='GW_FZXX' and con_table_id=wjbm and dzwjm like '正式件%.ofd' order by dzwj_id desc(结果集取第一条) </公文内容>
			<!-- 该节点可选 -->
			<扩展属性></扩展属性>
		</公文信息>
		<!-- 该节点可选 -->
		<附件集>
			<!-- 可以有多个附件节点 -->
			<附件>
				<附件名称>test.txt dzwjm</附件名称>
				<扩展名>txt dzwjlx</扩展名>
				<!-- 该节点可选 -->
				<附件标识></附件标识>
				<!-- 该节点可选 -->
				<附件大小>20313320 dzwj_size</附件大小>
				<附件内容>附件内容的二进制流 select * from gw_dzwj where con_table='GW_FZXX' and con_table_id=wjbm and dzwjm not like '%审批件%'</附件内容>
			</附件>
		</附件集>
		<!-- 该节点可选 -->
		<备注>备注说明</备注>
	</封装实体>
</公文封装包>