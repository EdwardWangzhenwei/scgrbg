<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE Forms SYSTEM "form_definition.dtd"><Forms>	<form id="hello" desp="添加文号">				<widgets>			<layout name="position1" type="BandLayout"				attr="groupSize:1;valign:top;align:center;">				<layout type="BandLayout" name="buttonlist1" class="headline"					width="100%">					<basic name="label1" label="添加文号" widget="Label" width="100%"						leftSpace="10px" topSpace="5px" color="#56451F" attr="font-weight:bold;" />				</layout>				<basic name="flag" widget="Hidden"></basic>				<layout name="query" type="TableLayout"					attr="groupSize:1;cellspacing:0;cellpadding:0;align:center;" width="100%"					class="in-table">					<basic name="GZMC" label="文号名称" widget="Edit" width="150px"						    require="true" />						    <basic name="dd" label="hehehehhehe" width="150px"  ></basic>					<basic name="DWDZ" label="单位代字" widget="Edit" width="150px"						 checkattr="max:50;"  />					<basic name="BMDZ" label="部门代字" widget="Edit" width="150px"						 checkattr="max:50;"  />					<layout type="BandLayout">					<basic name="FGWJ" widget="Label" 						label="法规文件"  />					<basic name="FGWJ" widget="Combox" attr="addnull:false;">						<valueFetcher							type="com.aisino.platform.view.basicWidget.fetcher.StaticFetcher">							<value name="1">否</value>							<value name="0">是</value>						</valueFetcher>					</basic>				  </layout>					<basic name="QTWZ" label="其他文字" widget="Edit" width="150px"						checkattr="max:50;"  />					<basic name="ssdwid" widget="Hidden" />					<basic name="HZ" label="字号后缀" widget="Edit" width="150px"						 />												<layout type="BandLayout">					<basic name="XSNH" widget="Label" 						label="是否有年"  />					<basic name="XSNH" widget="Combox" attr="addnull:false;">						<valueFetcher							type="com.aisino.platform.view.basicWidget.fetcher.SqlValueGetter">							<value name="sql">select gw.mbmc  from GW_MBGL gw</value>													</valueFetcher>					</basic>				</layout>				   	<basic name="WHXSGZ" label="显示文字" widget="Edit"></basic>					<!-- <basic name="WHXSGZ" label="显示文字" widget="Edit" width="150px"						 checkattr="max:50;" require="true" /> -->					<basic name="jygnid" widget="Hidden" />				</layout>			</layout>			<layout type="BandLayout" name="buttonBandLayout3" attr="align:center;"				topSpace="10px">				<basic name="save" label="保存" class="gradual-btnitem" width="60px"					height="25px" rightSpace="30px" widget="OButton" />				<basic name="back" label="关闭" class="gradual-btnitem"					width="60px" widget="OButton" height="25px" />			</layout>		</widgets>				<bind element="save" event="click">		<![CDATA[				PT.s('save');				PT.closeModal(null);			]]>				</bind>		<bind element="back" event="click"><![CDATA[				PT.closeModal(null);			]]></bind>	</form></Forms>