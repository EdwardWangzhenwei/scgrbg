<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE Forms SYSTEM "../form_definition.dtd"><Forms>	<form id="login" desp="公文管理系统">		<widgets>			<layout name="halign" type="BandLayout" width="100%" height="100%"				align="center">				<layout name="valign" type="TableLayout" width="690px"					height="100%" attr="valign:middle;">					<layout name="bkground" type="TableLayout" width="690px"						height="350px" backgroundImg="img/loginbg1.jpg" attr="valign:bottom;">						<layout name="position" align="center" type="TableLayout"							width="300px" height="280px" attr="groupSize:1;valign:middle;"							leftSpace="320px">							<layout name="content" type="TableLayout" attr="groupSize:1;cellspacing:10">								<basic name="username" label="登录名：" widget="Edit" width="150px"></basic>								<basic name="userpwd" label="密码：" widget="Password" width="150px"/>								<basic name="ok" leftSpace="90px" height="27px" width="93px"									widget="OButton" attr="buttonClass:picbutton;" backgroundImg="img/loginbtn.jpg" />							</layout>						</layout>					</layout>				</layout>			</layout>		</widgets>		<extendPoint>			<plugin type="cn.com.huadi.aos.hdoa.login.plugin.LoginPlugin"				submitEvent="login"></plugin>		</extendPoint>		<bind element="ok" event="click"><![CDATA[			if(wg('username') == '' || wg('username') == null )			{				PT.alert('登录名不能为空,请重新输入！'); 				return;			}			if(wg('userpwd') == '' || wg('userpwd') == null )			{				PT.alert('密码不能为空,请重新输入！'); 				return;			}		    var res = PT.s('login',null,null);		    if(res=='ok'){		      window.location.href="pt/res/hdoa/index.jsp";		    }		    		   else{		      PT.alert(res);		    }					]]></bind>	</form></Forms>				